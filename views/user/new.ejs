<%- include('../partials/header.ejs')%>
<br>
    <form action="/users" method="POST">
        <label for="firstName">First Name</label><br>
        <input type="text" id="firstName" name="firstName" class = "fixed-width" required><br><br>
        <label for="lastName">Last Name</label><br>
        <input type="text" id="lastName" name="lastName" class = "fixed-width" required><br><br>
        <label for="email">Email</label><br>
        <input type="email" id="email" name="email" class = "fixed-width" required><br><br>
        <label for="password">Password</label><br>
        <input type="password" id="password" name="password" class = "fixed-width" minlength="8" required><br><br>
        <button>Sign Up</button><br>
    </form>

</body>

<%- include('../partials/footer.ejs')%>